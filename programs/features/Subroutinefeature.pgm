program Simpleconverterscript;

var{
integer i;
}

FUNCTION FINAL action(integer j) returns integer{
VAR{
integer k;
}
Function subfunc(integer l) returns integer{
    DO{
        PRINTLN('This print statement is executed by calling action() which calls subfunc(), a subroutine defined in action(), - subFunc() calls this print statment!!!');
        PRINTLN('YAY THE SUBROUTINE FEATURE WORKS');
        return 0;
    }
}
}
Do{
    k = subfunc(0);
    return 0;
}
}
DO{
    i = action(0);
}
